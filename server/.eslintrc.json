{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": ["react", "jsx-a11y", "import", "babel", "flowtype"],
  "globals": {
    "SyntheticEvent": true,
    "SyntheticAnimationEvent": true,
    "SyntheticClipboardEvent": true,
    "SyntheticCompositionEvent": true,
    "SyntheticInputEvent": true,
    "SyntheticUIEvent": true,
    "SyntheticFocusEvent": true,
    "SyntheticKeyboardEvent": true,
    "SyntheticMouseEvent": true,
    "SyntheticDragEvent": true,
    "SyntheticWheelEvent": true,
    "SyntheticTouchEvent": true,
    "SyntheticTransitionEvent": true
  },
  "rules": {
    "no-shadow": 0,
    "no-underscore-dangle": [2, { "allow": ["__"] }],
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": "off",
    "arrow-body-style": "off",
    "no-plusplus": "off",
    "no-confusing-arrow": "off",
    "no-unused-expressions": "off",
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "[Rr]eact"
      }
    ],
    "no-nested-ternary": 0,
    "react/require-default-props": 0,
    "no-return-assign": 0,
    "import/no-extraneous-dependencies": 0
  }
}
